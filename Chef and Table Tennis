#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    
	    int count1=0,count0=0;
	    for(int i=0;i<s.length();i++){
	        if(s[i]=='1'){
	            count1++;
	        }
	        else{
	            count0++;
	        }
	    }
	    
	    if(count1>count0){
	        cout<<"WIN"<<endl;
	    }
	    else{
	        cout<<"LOSE"<<endl;
	    }
	}
	
	return 0;
}
